{
{%- for property in properties %}
  /**
    * {{ property.name }}
{%- if property.example %}
    * Example: {{ property.example }}
{%- endif %}
    */
  {{ property.name }}{{ '?' if not property.required }}:

{%- if property.types == ['object'] -%}
    {% with properties=property.properties %}
        {% include 'typescript.object.jinja2' %}
    {%- endwith -%}
{%- else -%}
    {%- if property.enum -%}
        {{ property.enum|join(' | ') }}
    {%- else -%}
        {{ property.types|join(' | ') }}
    {%- endif -%}
{%- endif -%}

    {%- if property.is_array -%}
        []
    {%- endif -%};

{%- endfor %}
}
