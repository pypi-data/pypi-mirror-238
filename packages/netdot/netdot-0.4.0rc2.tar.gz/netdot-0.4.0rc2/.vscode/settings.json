{
    "python.testing.pytestArgs": [
        "-v",
        "--doctest-modules"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.nosetestsEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.enabled": true,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--ignore",
        "E201,E202,E226,E231,E302,E401,E501,W291,W503"
    ],
    "cSpell.words": [
        "assertpy",
        "capsys",
        "contactlist",
        "examplepkg",
        "faceplateid",
        "fwtable",
        "fwtableentries",
        "horizontalcable",
        "ipblock",
        "jackid",
        "Jenkinsfile",
        "Maint",
        "mgmt",
        "mkdocs",
        "monitorstatus",
        "netdot",
        "NTS",
        "ntsjenkins",
        "pathvalidate",
        "physaddr",
        "pobox",
        "producttype",
        "pypi",
        "pytest",
        "rraddr",
        "RRCNAME",
        "RRDS",
        "RRHINFO",
        "RRLOC",
        "RRMX",
        "RRNAPTR",
        "RRNS",
        "RRPTR",
        "RRSRV",
        "RRTXT",
        "setuptools",
        "snmp",
        "testpassed",
        "unitofwork",
        "uologging",
        "venv",
        "xlink",
        "xlinks"
    ],
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.egg-info": true,
        "**/.pytest_cache": true,
    }
}
