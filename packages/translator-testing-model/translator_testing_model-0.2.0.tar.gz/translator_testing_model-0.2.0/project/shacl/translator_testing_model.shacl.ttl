        [ sh:in ( "Top_Answer" "Acceptable" "BadButForgivable" "NeverShow" "number_1_TopAnswer" "number_2_Acceptable" "number_3_BadButForgivable" "number_4_NeverShow" ) ;
            sh:maxCount 1 ;
            sh:order 15 ;
            sh:path ttm:expected_output ],
        [ sh:datatype xsd:string ;
