                }
            },
            "required": [
                "bins"
            ],
            "title": "Welcome5"
        },
        "Bin": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "type": {
                    "$ref": "#/definitions/Type"
                },
                "collectionDate": {
                    "type": "string"
                }
            },
            "required": [
                "collectionDate",
                "type"
            ],
            "title": "Bin"
        },
        "Type": {
            "type": "string",
