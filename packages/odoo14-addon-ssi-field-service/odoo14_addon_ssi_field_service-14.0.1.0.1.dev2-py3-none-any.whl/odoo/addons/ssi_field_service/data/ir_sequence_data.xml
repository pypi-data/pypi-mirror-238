<odoo>
<data noupdate="1">
<record id="sequence_field_service_order" model="ir.sequence">
    <field name="name">Field Service Order</field>
    <field name="code">field_service_order</field>
    <field name="prefix">FSO/%(range_y)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>

<record id="sequence_group_field_service_order" model="ir.sequence">
    <field name="name">Group Field Service Order</field>
    <field name="code">group_field_service_order</field>
    <field name="prefix">GFSO/%(range_y)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>
</data>
</odoo>
