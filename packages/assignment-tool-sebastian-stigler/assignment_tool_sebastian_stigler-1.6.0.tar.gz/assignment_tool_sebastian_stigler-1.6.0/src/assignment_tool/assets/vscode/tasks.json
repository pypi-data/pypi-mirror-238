{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Testat kompilieren",
			"command": "/usr/bin/gcc",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"*.c",
				"-o",
				"${fileBasenameNoExtension}",
				"-Wall"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "Compiler: /usr/bin/gcc"
		}
	]
}
